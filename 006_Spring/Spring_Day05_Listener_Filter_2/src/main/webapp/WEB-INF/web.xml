<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>Spring_Day05_Listener_Filter_2</display-name>

  <listener>
    <listener-class>com.ssafy.listener.MyListener</listener-class>
  </listener>
  <context-param>
    <param-name>welcome</param-name>
    <param-value>Hello JW</param-value>
  </context-param>
 
  
  <filter>
    <display-name>MyFilter</display-name>
    <filter-name>MyFilter</filter-name>
    <filter-class>com.ssafy.filter.MyFilter</filter-class>
    <init-param>
    	<param-name>encoding</param-name>
    	<param-value>UTF-16</param-value>
    </init-param>
  </filter>
  
  <filter-mapping>
    <filter-name>MyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  
</web-app>